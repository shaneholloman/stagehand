{
  "name": "@browserbasehq/stagehand-evals",
  "version": "1.1.6",
  "private": true,
  "description": "Evaluation suite for Stagehand",
  "main": "./",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "pnpm run typecheck",
    "build:cli": "tsup cli.ts --outDir ../../dist/evals --format cjs && cp evals.config.json ../../dist/evals/ && chmod +x ../../dist/evals/cli.js",
    "evals": "tsx run.ts",
    "lint": "cd ../.. && prettier --check packages/evals && cd packages/evals && eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@browserbasehq/stagehand": "workspace:*",
    "ai": "^5.0.0",
    "dotenv": "16.4.5",
    "openai": "^4.87.1",
    "sharp": "^0.34.5",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "braintrust": "^0.4.7",
    "chalk": "^5.4.1",
    "string-comparison": "^1.3.0",
    "tsx": "^4.10.5"
  }
}
